  "authors": [
    "Fallen_Breath"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/classic-gamerule",
    "sources": "https://github.com/Fallen-Breath/classic-gamerule"
  },

  "icon": "assets/classicgamerule/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.fallenbreath.classicgamerule.ClassicGameruleMod"
    ]
  },
  "mixins": [
    "classicgamerule.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.4",
    "minecraft": "${minecraft_dependency}"
  }
}
